{
  "version": 2,
  "builds": [
    {
      "src": "src/manage.py",
      "use": "@vercel/python",
      "config": {
        "distDir": "staticfiles"
      }
    },
    {
      "src": "src/pedigroup/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/pedigroup/wsgi.py"
    }
  ]
}
