{
  "version": 2,
  "builds": [
    {
      "src": "src/manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/pedigroup/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "src/manage.py" }
  ],
  "env": {
    "TELEGRAM_BOT_TOKEN": "@telegram_bot_token"
  }
}
