{
  "version": 2,
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "src/settings.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/telegram-webhook/",
      "dest": "/src/views.py",
      "methods": ["POST"]
    }
  ]
}
